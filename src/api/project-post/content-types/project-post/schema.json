{
  "kind": "collectionType",
  "collectionName": "project_posts",
  "info": {
    "singularName": "project-post",
    "pluralName": "project-posts",
    "displayName": "Project Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "projectSummary": {
      "type": "text"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "project_posts"
    },
    "projectUrl": {
      "type": "string"
    },
    "projectImages": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "technologies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::technology.technology",
      "mappedBy": "project_posts"
    }
  }
}
